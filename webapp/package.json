{
  "name": "mercurio-checkout",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mundipagg/mercurio-checkout.git"
  },
  "homepage": "https://github.com/mundipagg/mercurio-checkout",
  "bugs": "https://github.com/mundipagg/mercurio-checkout/issues",
  "dependencies": {
    "@storybook/react": "3.2.16",
    "autoprefixer": "7.1.2",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "3.0.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "caniuse-lite": "1.0.30000697",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "eslint": "4.11.0",
    "eslint-config-pagarme-react": "1.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "21.2.1",
    "postcss-cssnext": "3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.8",
    "postcss-mixins": "6.2.0",
    "postcss-sass-each": "0.1.0",
    "postcss-url": "7.2.1",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dev-utils": "4.1.0",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-test-renderer": "16.0.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "style-loader": "0.19.0",
    "stylelint": "8.2.0",
    "stylelint-config-pagarme-react": "1.1.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "eslint './src/**/*.js' && stylelint './src/**/*.css'",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "storybook": "start-storybook -p 6006"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
